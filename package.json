{
  "name": "react-designable-audio-player",
  "version": "0.1.0",
  "description": "An audio player library for React that lets you use your own UI component",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src tests",
    "build:examples": "cd examples; npm uninstall react-designable-audio-player; npm install kenfehling/react-designable-audio-player; npm run build",
    "build:module": "webpack ./src/index.js --config webpack.config.development.babel.js",
    "build:module:min": "webpack ./src/index.js --config webpack.config.production.babel.js",
    "build": "npm run build:module:min",
    "publish:examples": "git subtree push --prefix examples/build origin gh-pages",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "stats": "webpack --config webpack.config.production.babel.js --profile --json > stats.json"
  },
  "author": "Ken Fehling",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenfehling/react-designable-audio-player"
  },
  "bugs": "https://github.com/kenfehling/react-designable-audio-player/issues",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "audio",
    "player",
    "hoc"
  ],
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "insert-css": "^2.0.0",
    "prop-types": "^15.5.8",
    "rc-slider": "^10.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-jest": "^16.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.3.0",
    "fbjs": "^3.0.4",
    "jest-cli": "^15.1.1",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.3",
    "rimraf": "^2.3.4",
    "style-loader": "^0.13.1",
    "webpack": "^5.0.0",
    "webpack-babel-jest": "^1.0.4",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/webpack-babel-jest",
    "testPathDirs": [
      "tests"
    ],
    "testRegex": "/*.test.js$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  }
}

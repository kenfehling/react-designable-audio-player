{
  "name": "react-designable-audio-player",
  "version": "0.1.1",
  "description": "An audio player library for React that lets you use your own UI component",
  "source": "src/index.js",
  "main": "dist/main.mjs",
  "targets": {
    "main": {
      "source": "src/index.js",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "includeNodeModules": ["fbjs", "insert-css", "rc-slider"]
    }
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src tests",
    "build:examples": "cd examples; npm uninstall react-designable-audio-player; npm install kenfehling/react-designable-audio-player; npm run build",
    "watch": "parcel watch",
    "build": "parcel build",
    "publish:examples": "git subtree push --prefix examples/build origin gh-pages",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Ken Fehling",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenfehling/react-designable-audio-player"
  },
  "bugs": "https://github.com/kenfehling/react-designable-audio-player/issues",
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "audio",
    "player",
    "hoc"
  ],
  "dependencies": {
    "fbjs": "^3.0.4",
    "insert-css": "^2.0.0",
    "prop-types": "^15.5.8",
    "rc-slider": "^10.0.1"
  },
  "devDependencies": {
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.3.0",
    "jest-cli": "^15.1.1",
    "parcel": "^2.7.0",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.3",
    "rimraf": "^2.3.4"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/webpack-babel-jest",
    "testPathDirs": [
      "tests"
    ],
    "testRegex": "/*.test.js$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  }
}
